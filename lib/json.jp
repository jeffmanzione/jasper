import struct

def jsonify(value) {
  if value is Object {
    return _jsonify_object(value)
  }
  return stringify(value)
}

def _jsonify_object(obj) {
  if obj is String {
    return '\"' + obj + '\"'
  } else if obj is Array {
    return _jsonify_array(obj)
  } else if obj is struct.Map {
    return _jsonify_map(obj)
  }
  
}


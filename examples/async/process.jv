import io
import async

class Test {
  field a, b, c
  new() {
    a = [1, 2, 3, 4, 5]
    b = 12
    c = (a, b)
  }
  method to_s() {
    str('Spawned process ', a, b, c)
  }
}

; Test object is copied to the new process.
p = async.create_process((t) -> io.println(t), Test())

f = p.start()

io.println('Original process.')

await f